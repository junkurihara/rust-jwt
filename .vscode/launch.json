  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rjwt'",
      "cargo": {
        "args": [
          "build",
          "--bin=rjwt",
          "--package=rjwt"
        ],
        "filter": {
          "name": "rjwt",
          "kind": "bin"
        }
      },
      "env": {
        "RUST_LOG": "debug"
      },
      "args": [
        "test_subject"
      ],
      "cwd": "${workspaceFolder}/.private"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'rjwt'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=rjwt",
          "--package=rjwt"
        ],
        "filter": {
          "name": "rjwt",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
